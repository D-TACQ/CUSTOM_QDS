# RW diamond
record(longout, "${UUT}:QDS:CHANSEL:${xx}") {
	field(DTYP, "Soft Channel")
	field(LOPR, "0")
	field(HOPR, "16")
	field(DESC, "Select CH for Y$xx")
	field(FLNK, "${UUT}:QDS:CHANSEL:${xx}:w PP")	
	field(PINI, "YES")
	field(DOL,  "${xx}")
	field(DRVL, "0")
	field(DRVH, "16")
}

record(longout, "${UUT}:QDS:CHANSEL:${xx}:w") {
	field(DTYP, "stream")
	field(OUT,  "@qds.proto set_chansel(${xx}) ${SPORT}")
	field(DISV, "1")
	field(SDIS, "${UUT}:QDS:CHANSEL:${xx}:r.PACT")
	field(OMSL, "closed_loop")
	field(DOL,  "${UUT}:QDS:CHANSEL:${xx}")
}

record(longin, "${UUT}:QDS:CHANSEL:${xx}:r") {
	field(DTYP, "stream")
	field(INP,  "@qds.proto get_chansel(${xx}) ${SPORT}")
	field(SCAN, "1 second")
	field(FLNK, "${UUT}:QDS:CHANSEL:${xx}:s")
}

record(longout, "${UUT}:QDS:CHANSEL:${xx}:s") {
	field(DTYP, "Soft Channel")
	field(OMSL, "closed_loop")
	field(DOL,  "${UUT}:QDS:CHANSEL:${xx}:r NPP")
	field(OUT,  "${UUT}:QDS:CHANSEL:${xx}")
}

record(ai, "${UUT}:QDS:X${xx}") {
	field(DTYP, "stream")
	field(INP,  "@qds.proto get_x(${xx}) ${SPORT}")
	field(SCAN, ".1 second")
	field(DESC, "instantaneous X value")
	field(EGU,  "V")
	field(PREC,  "3")
	field(EOFF, "0")
	field(EGUL, "-100")
	field(EGUF, "+9.999")
	field(LINR, "LINEAR")
	field(EGU,  "Volts")
	field(ESLO, "3.0518e-4")
	field(HOPR, "9.999")
	field(LOPR, "-10.000")	
}